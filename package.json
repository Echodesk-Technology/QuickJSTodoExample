{
  "name": "QuickjsTodoexample",
  "version": "1.0.0",
  "description": "Create Quick App",
  "private": "true",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "quickstart": "npm install",
    "dev": "nodemon server.js",
    "develop": "webpack --config webpack.dev.js",
    "serve": "concurrently --kill-others \"npm run dev\" \"npm run develop\"",
    "build": "npm install webpack && webpack --config webpack.prod.js",
    "build:css": "tailwind build src/assets/css/build.css -o src/assets/css/main.css"
  },
